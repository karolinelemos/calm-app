var ReactNative = require('react-native');
var React = require('react');

var _require = require('./menu')(React, ReactNative),
    Menu = _require.Menu,
    MenuContext = _require.MenuContext,
    MenuOptions = _require.MenuOptions,
    MenuOption = _require.MenuOption,
    MenuTrigger = _require.MenuTrigger;

Menu.MenuContext = MenuContext;
Menu.MenuOption = MenuOption;
Menu.MenuOptions = MenuOptions;
Menu.MenuTrigger = MenuTrigger;

module.exports = Menu;